import material/extra/tree_editor/tree_editor;

main() {
	manager = makeMaterialManager([]);
	getNewNode = \ -> makeMTreeEditorNode(makeTropicTag(manager), TEmpty());
	tree = makeMTreeEditorNode(makeTropicTag(manager), TEmpty());
	editor = makeMTreeEditor(manager, tree, \cb -> cb(getNewNode()), []);

	mrender(
		manager,
		true,
		MLines2(
			editor.toolbar,
			editor.view
		)
	)
	|> ignore;
}