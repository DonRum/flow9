import ds/array;

Hypothesis ::= Essential, Floating;
	Essential();
	Floating();

Expr(symbs : [string]);

TheoryEnv(
	hyps : [Hypothesis],
);

makeAssertion(env : TheoryEnv) -> [Expr] {
	// essentials must have [Essential] type, not [Expr] !
	essentials : [Expr] = filter(env.hyps, 
		\hyp : Hypothesis -> switch(hyp) {
			Essential(): true;
			default: false;
		}
	);
	essentials
}
