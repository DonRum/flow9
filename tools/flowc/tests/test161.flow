
	TableFilter<?> ::= TableStringFilter<?>, TableIntFilter<?>;
		TableStringFilter(
			name : string,
			defValue : string,
			valueB : DynamicBehaviour<string>,
			pebbleValueB : DynamicBehaviour<string>,
			value2String : (string) -> string,
			applicableValue : (?, string) -> bool,
			onPebbleValueBChange : (DynamicBehaviour<string>, string, string) -> void,
		);
		TableIntFilter(
			name : string,
			defValue : int,
			valueB : DynamicBehaviour<int>,
			pebbleValueB : DynamicBehaviour<int>,
			value2String : (int) -> string,
			applicableValue : (?, int) -> bool,
			onPebbleValueBChange : (DynamicBehaviour<int>, int, int) -> void,
		);

DynamicBehaviour(value : ?);
getValue(b : DynamicBehaviour<?>) -> ? {
	b.value;
}

forall(a : [?], fn : (?) -> bool) -> bool {
	true;
}


isInTableFilters(filters : [TableFilter<?>], x : ??) -> bool {
	forall(filters, \f : TableFilter<?> -> f.defValue == getValue(f.valueB) 
		|| f.applicableValue(x, getValue(f.valueB))
	);
}

