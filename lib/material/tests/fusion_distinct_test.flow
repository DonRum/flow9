import material/material_manager;
import material/material2tropic;

main() {
	f1 = make("a");
	f2 = fselect(f1, FLift(\ff1 -> {
		println("f1 = " + ff1);
		"b"
	}));

	en = fselect2(
		f2,
		fselect2(f1, f2, FLift2(\__, __ -> "")),
		FLift2(\__, __ -> false)
	);
	
	m = MCheckBox(MEmpty(), make(false), [MEnabled(en)]);
	mrender(makeMaterialManager([]), false, m);
	
	quit(0);
}
